<!DOCTYPE html>
<html lang="en">
<head>
{{> includes/head}}
</head>
<body>
    {{> includes/header}} 
  
    <section id="services" class="space-top-2x space-bottom-2x">
        <div class="container">
            <div class="row">
                {{#services}}
                <div class="col-xs-6 col-lg-4 feature-center">
                    <div class="feature block">
                        <div class="feature-icon">
                            <i class="fa fa-{{icon}}"></i>
                        </div>
                        <h5>{{title}}</h5>
                        <p>{{description}}</p>
                    </div>
                </div>
                {{/services}}
            </div>
        </div>
    </section>

    {{> includes/footer}}
    {{> includes/footerScripts}}
</body>
</html>