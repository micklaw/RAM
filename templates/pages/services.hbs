<!DOCTYPE html>
<html lang="en">
<head>
{{> head}}
</head>
<body>
    {{> header}} 
  
    <section id="services" class="space-top-2x space-bottom-2x">
        <div class="container">
            <div class="row">
                {{#body.services}}
                <div class="col-xs-6 col-lg-4 feature-center">
                    <div class="feature block">
                        <div class="feature-icon">
                            <i class="fa fa-{{icon}}"></i>
                        </div>
                        <h5>{{title}}</h5>
                        <p>{{{description}}}</p>
                    </div>
                </div>
                {{/body.services}}
            </div>
        </div>
    </section>

    {{> footer}}
    {{> footerScripts}}
</body>
</html>